extends layouts/main.pug

block content
    .event-container
        img(src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRw-yn5P_1s-1aJiBGhPo4W5pUtUPBi-57cbA&usqp=CAU")
    .goods
        each item in goods
            a(href=routes.goodsDetail(item.id))
                h1= item.title
                h2= item.description
                h3= item.pays
                img(src=item.fileUrl)